 //将一个二维数组行和列元素互换，存到另一个二位数组中
int main()
{
	int arr1[2][3] = { 0}; //定义一个两行三列的数组
	int arr2[3][2] = {0}; //定义一个三行两列的数组
	printf("请输入6个数:\n");
	for (int i= 0;i<2;i++)
	{
		for (int j = 0;j < 3;j++)
		{
			scanf("%d",&arr1[i][j]); //初始化arr1的值
			printf("%d\t", arr1[i][j]);
			arr2[j][i] = arr1[i][j];
		}
		printf("\n");
	}
	printf("\n");
	printf("\n");
	printf("\n");
	for (int i = 0;i <3;i++)
	{
		for (int j = 0;j < 2;j++)
		{
			printf("%d\t",arr2[i][j]);
		}
		printf("\n");
	}
	return 0;
}
